--Create a shoe store database

create table shoes (id integer primary key, name text, size integer, quantity integer, price integer);
INSERT INTO shoes VALUES (1, "Tennis Shoes", 7, 3, 15);
INSERT INTO shoes VALUES (2, "Heels", 6, 2, 20);
INSERT INTO shoes VALUES (3, "Sandles", 8, 6, 10);
INSERT INTO shoes VALUES (4, "Crocs", 6, 4, 25);
INSERT INTO shoes VALUES (5, "Wedges", 9, 5, 20);

--display the database ordered by price.
SELECT * FROM shoes;
SELECT * FROM shoes where price ORDER BY price;
SELECT id, sum(quantity) FROM shoes;
